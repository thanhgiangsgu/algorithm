#include <iostream>

using namespace std;

int main(){
    int t;
    int n, k;
    cin >> t;
    for (int i = 1 ; i <= t; i++){
        cin >> n >> k;
        int count = 0;
        int tmp;
        for (int j = 1; j<= n;j++)
        {
            cin >> tmp;
            if (tmp <= 0) count++;
        }
        if (count >= k) cout<<"NO"<<endl; else 
            cout<<"YES"<<endl;
    }
}
